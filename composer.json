{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/api-pack": "^1.2",
        "aws/aws-sdk-php": "^3.140",
        "doctrine/doctrine-migrations-bundle": "^2.1",
        "knplabs/knp-gaufrette-bundle": "^0.7.1",
        "league/oauth2-server": "^8.1",
        "nelmio/cors-bundle": "^2.0",
        "nyholm/psr7": "^1.3",
        "ramsey/uuid-doctrine": "^1.6",
        "stof/doctrine-extensions-bundle": "^1.4",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.1.*",
        "symfony/http-client": "5.1.*",
        "symfony/mailer": "5.1.*",
        "symfony/messenger": "5.1.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/yaml": "5.1.*",
        "trikoder/oauth2-bundle": "^3.1",
        "vich/uploader-bundle": "^1.13"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "hautelook/alice-bundle": "^2.7",
        "lchrusciel/api-test-case": "^5.0",
        "symfony/maker-bundle": "^1.19",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "db-migrate": [
            "bin/console doctrine:migrations:migrate -n"
        ],
        "db-recreate": [
            "bin/console doctrine:database:drop --force",
            "bin/console doctrine:database:create",
            "bin/console doctrine:migrations:migrate -n"
        ],
        "db-fixtures": [
            "@db-migrate",
            "bin/console hautelook:fixtures:load -n"
        ],
        "phpunit": [
            "bin/console doctrine:database:create --if-not-exists --env=test -vvv",
            "bin/console doctrine:migrations:migrate --no-interaction --env=test --no-debug -vvv",
            "vendor/bin/phpunit -vvv"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
